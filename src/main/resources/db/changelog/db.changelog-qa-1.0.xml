<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="insertDefaultTemplate" author="renato.ibanhez" context="qa, local">
        <insert tableName="feed_template" >
            <column name="id" valueComputed="${uuid_function}"/>
            <column name="slug" value="default_template"/>
            <column name="name" value="default_template"/>
            <column name="template" value="default_template"/>
            <column name="separator" value=">"/>
            <column name="format" value="default_template"/>
        </insert>
    </changeSet>

</databaseChangeLog>